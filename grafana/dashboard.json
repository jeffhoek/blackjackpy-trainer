      "type": "stat",
      "targets": [
        {
          "query": "correct = from(bucket: \"jenkins\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"blackjack_answer_correct\" and r._field == \"value\")\n  |> group()\n  |> sum()\n\nwrong = from(bucket: \"jenkins\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"blackjack_answer_wrong\" and r._field == \"value\")\n  |> group()\n  |> sum()\n\njoin(tables: {correct: correct, wrong: wrong}, on: [\"_stop\"])\n  |> map(fn: (r) => ({\n    r with\n    _value: float(v: r._value_correct) / (float(v: r._value_correct) + float(v: r._value_wrong)) * 100.0\n  }))",
