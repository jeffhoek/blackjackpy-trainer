          "query": "import \"strings\"\n\ncorrect = from(bucket: \"jenkins\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement =~ /^blackjack_answer_hand_type_.*_correct$/ and r._field == \"value\")\n  |> group(columns: [\"_measurement\"])\n  |> sum()\n  |> map(fn: (r) => ({\n    r with\n    hand_type: strings.trimSuffix(v: strings.trimPrefix(v: r._measurement, prefix: \"blackjack_answer_hand_type_\"), suffix: \"_correct\")\n  }))\n  |> group()\n\nwrong = from(bucket: \"jenkins\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement =~ /^blackjack_answer_hand_type_.*_wrong$/ and r._field == \"value\")\n  |> group(columns: [\"_measurement\"])\n  |> sum()\n  |> map(fn: (r) => ({\n    r with\n    hand_type: strings.trimSuffix(v: strings.trimPrefix(v: r._measurement, prefix: \"blackjack_answer_hand_type_\"), suffix: \"_wrong\")\n  }))\n  |> group()\n\njoin(tables: {correct: correct, wrong: wrong}, on: [\"hand_type\"])\n  |> map(fn: (r) => ({\n    hand_type: r.hand_type,\n    _value: float(v: r._value_correct) / (float(v: r._value_correct) + float(v: r._value_wrong)) * 100.0\n  }))\n  |> rename(columns: {_value: \"accuracy\"})",
